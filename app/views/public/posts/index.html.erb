<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>投稿者</th>
      <th>投稿タイトル</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td>
          <%= post.user.name %>
        </td>
        <td><%= link_to post.title,post %></td>
    <% end %>
  </tbody>
</table>